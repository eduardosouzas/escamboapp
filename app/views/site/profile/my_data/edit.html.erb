<h1 class="header">Editando Profile Member</h1>
<div class="center-block">
    <%= form_for(@profile_member, url: site_profile_my_datum_path(@profile_member) ) do |f| %>
    <div class="row">
      <div class="col-xs-6">
          <div class="form-group">
            <%= f.label :first_name %>
            <%= f.text_field :first_name, class:"form-control", placeholder:"Nome"  %>
          </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-6">
          <div class="form-group">
            <%= f.label :second_name %>
            <%= f.text_field :second_name, class:"form-control", placeholder:"Sobrenome"  %>
          </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-2">
          <div class="form-group">
            <%= f.label :birthdate %>
            <%= f.text_field :birthdate, class:"form-control datepicker" ,
            value: (l(@profile_member.birthdate) unless @profile_member.birthdate.blank? )  %>
          </div>
      </div>
    </div>
      <%= submit_tag "Enviar", class:"btn btn-primary" %>
    <% end %>
</div>
